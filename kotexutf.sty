  % redefine footref for autojosa (footmisc v5.3d)
  \@ifpackageloaded{footmisc}{%
    \if@nojosa\else
      \renewcommand\footref[1]{%
        \begingroup
          \let\@setref\@setref@dhucs@orig@
          \unrestored@protected@xdef\@thefnmark{\ref{#1}}%
