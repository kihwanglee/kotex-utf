            \@ifundefined{b@\@citeb\@extra@b@citeb}{}{%
              \edef\@temp{\csname b@\@citeb\@extra@b@citeb\endcsname}}}%
        \fi
        \global\josatoks\expandafter{\@temp}%
        \endgroup}%
